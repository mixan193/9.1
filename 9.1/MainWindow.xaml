<Window x:Class="_9._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_9._1"
        mc:Ignorable="d"
        Title="9.1" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="390"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Margin="405,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="385"/>
        <Button x:Name="button1" Content="1 функция" HorizontalAlignment="Left" Margin="10,33,0,0" VerticalAlignment="Top" Width="390" Click="button1_Click"/>
        <Button x:Name="button2" Content="2 функция" HorizontalAlignment="Left" Margin="405,33,0,0" VerticalAlignment="Top" Width="385" Click="button2_Click"/>
        <ListBox x:Name="listBox1" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,58,0,34" HorizontalAlignment="Left" Width="390"/>
        <Label x:Name="label1" HorizontalAlignment="Left" Margin="405,58,0,0" VerticalAlignment="Top" Width="385"/>

    </Grid>
</Window>
